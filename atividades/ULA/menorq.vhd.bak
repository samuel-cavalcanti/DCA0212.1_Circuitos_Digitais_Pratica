library ieee;
use ieee.std_logic_1164.all;
use ieee.numeric_std.all;

entity menorque is
	port(inputdata1: in std_logic_vector (3 downto 0);
		  inputdata2: in std_logic_vector (3 downto 0);
		  resulmeq: out std_logic;);
end entity;

architecture behavior of menorque is
begin
	process(inputdata1, inputdata2)
		variable meq: std_logic;
		begin
			if inputdata2(3) > inputdata1(3) then
				meq := 1; -- inputdata1 Ã© o maior
			elsif inputdata2(2) > inputdata1(2) then
				meq := 1;
			elsif inputdata2(1) > inputdata1(1) then
				meq := 1;
			elsif inputdata2(0) > inputdata1(0) then
				meq := 1;
			end if;
	end process;
end behavior;