module set_clock_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk_in;
// wires                                               
wire clk_out;
wire [7:0] contador;

wire sampler;                             

// assign statements (if any)                          
set_clock i1 (
// port map - connection between master ports and signals/registers   
	.clk_in(clk_in),
	.clk_out(clk_out),
	.contador(contador)
);

// clk_in
always
begin
	clk_in = 1'b0;
	clk_in = #10000 1'b1;
	#10000;
end 

set_clock_vlg_sample_tst tb_sample (
	.clk_in(clk_in),
	.sampler_tx(sampler)
);

set_clock_vlg_check_tst tb_out(
	.clk_out(clk_out),
	.contador(contador),
	.sampler_rx(sampler)
);
endmodule

